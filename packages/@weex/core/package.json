{
  "name": "@weex-cli/core",
  "version": "1.0.0-beta.7",
  "description": "Core for weex-toolkit",
  "main": "index.js",
  "scripts": {
    "build": "yarn format && yarn clean && yarn compile",
    "clean": "rm -rf ./lib",
    "compile": "tsc -p ./",
    "coverage": "jest --coverage",
    "format": "prettier --write \"**/*.ts\" --loglevel error && tslint -p . --fix",
    "lint": "tslint -p .",
    "publish": "yarn build && yarn lint && np --yolo --no-yarn",
    "precommit": "lint-staged",
    "snapupdate": "jest --updateSnapshot",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "tsc -watch -p ./"
  },
  "author": "Erha19 <faterrole@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apisauce": "^0.15.1",
    "app-module-path": "^2.2.0",
    "async": "^2.6.1",
    "cli-table2": "^0.2.0",
    "colors": "^1.3.0",
    "consolidate": "^0.15.1",
    "cosmiconfig": "^5.0.5",
    "cross-spawn": "^6.0.5",
    "download-git-repo": "^1.0.2",
    "enquirer": "^1.0.3",
    "execa": "^0.10.0",
    "fs-jetpack": "^1.3.0",
    "handlebars": "^4.0.11",
    "inquirer": "^6.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.lowercase": "^4.3.0",
    "lodash.lowerfirst": "^4.3.1",
    "lodash.pad": "^4.5.1",
    "lodash.padend": "^4.6.1",
    "lodash.padstart": "^4.6.1",
    "lodash.repeat": "^4.1.0",
    "lodash.snakecase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "lodash.trim": "^4.5.1",
    "lodash.trimend": "^4.5.1",
    "lodash.trimstart": "^4.5.1",
    "lodash.uppercase": "^4.3.0",
    "lodash.upperfirst": "^4.3.1",
    "metalsmith": "^2.3.0",
    "minimatch": "^3.0.4",
    "multimatch": "^2.1.0",
    "npminstall": "^3.6.2",
    "ora": "^2.1.0",
    "path-exists": "^3.0.0",
    "pluralize": "^7.0.0",
    "prompt-autocompletion": "^0.1.1",
    "prompt-checkbox": "^2.2.0",
    "prompt-confirm": "^1.2.0",
    "prompt-expand": "^1.0.1",
    "prompt-list": "^3.1.0",
    "prompt-password": "^1.2.0",
    "prompt-question": "^5.0.2",
    "prompt-radio": "^1.2.1",
    "prompt-rawlist": "^2.0.1",
    "ramda": "^0.25.0",
    "semver": "^5.5.0",
    "validate-npm-package-name": "^3.0.0",
    "yargs-parser": "^10.0.0"
  },
  "devDependencies": {
    "@types/cli-table2": "^0.2.1",
    "@types/colors": "^1.2.1",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.4",
    "@types/pluralize": "^0.0.28",
    "@types/ramda": "^0.25.32",
    "coveralls": "^3.0.1",
    "expect": "^23.1.0",
    "jest": "^23.1.0",
    "lint-staged": "^7.1.3",
    "np": "^3.0.4",
    "prettier": "^1.14.0",
    "ramdasauce": "^2.1.0",
    "sinon": "^5.1.0",
    "temp-write": "^3.4.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^3.0.1",
    "unique-temp-dir": "^1.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)(test)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weexteam/weex-toolkit/issues"
  },
  "keywords": [
    "core"
  ]
}
